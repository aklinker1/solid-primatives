{
  "$schema": "https://deno.land/x/deno/cli/schemas/config-file.v1.json",
  "name": "@aklinker1/solid-primatives",
  "version": "0.3.18",
  "license": "MIT",
  "exports": {
    "./computed": "./src/computed.ts",
    "./createGlobalState": "./src/createGlobalState.ts",
    "./createSharedHook": "./src/createSharedHook.ts",
    "./defineComponent": "./src/defineComponent.ts",
    "./query": "./src/query.ts",
    "./readonly": "./src/readonly.ts",
    "./ref": "./src/ref.ts",
    "./setter": "./src/setter.ts",
    "./shallowRef": "./src/shallowRef.ts",
    "./toValue": "./src/toValue.ts",
    "./useDebounce": "./src/useDebounce.ts",
    "./useLocalStorage": "./src/useLocalStorage.ts",
    "./useRefHistory": "./src/useRefHistory.ts",
    "./useRequiredContext": "./src/useRequiredContext.ts",
    "./useSessionStorage": "./src/useSessionStorage.ts",
    "./useStorage": "./src/useStorage.ts",
    "./useThrottle": "./src/useThrottle.ts",
    "./useWindowEventListener": "./src/useWindowEventListener.ts",
    "./watch": "./src/watch.ts",
    "./watchIgnorable": "./src/watchIgnorable.ts",
    "./whenever": "./src/whenever.ts"
  },
  "lint": {
    "rules": {
      "exclude": ["no-explicit-any"]
    }
  },
  "publish": {
    "include": ["README.md", "src/**/*.ts", "src/**/*.tsx"],
    "exclude": ["**/*.test.ts"]
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "npm:solid-js@1"
  },
  "imports": {
    "solid-js": "npm:solid-js@^1.9.3"
  }
}
